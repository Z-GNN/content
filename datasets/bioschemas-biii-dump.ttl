        <http://edamontology.org/operation_Morphological_operations>,
        <http://edamontology.org/operation__RBNng13hEB0CJD5VzSKqrfB> ;
    ns1:license "GNU General Public License v3.0" ;
    ns1:name "MIA" ;
    ns1:publisher "Stephen Cross" ;
    ns1:softwareRequirements <http://biii.eu/node/1057>,
        <http://biii.eu/node/79> .

