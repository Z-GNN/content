<http://biii.eu/basic> a ns1:SoftwareApplication ;
    ns1:applicationCategory <http://edamontology.org/operation_Image_reconstruction>,
        <http://edamontology.org/operation_Image_stitching> ;
    ns1:citation <https://www.nature.com/articles/ncomms14836>,
        "A BaSiC tool for background and shading correction of optical microscopy images, Nat. Comm." ;
    ns1:dateCreated "2022-04-21T13:49:31" ;
    ns1:dateModified "2022-05-18T07:36:43" ;
    ns1:description """<p>BaSiC is a software tool for Background and Shading correction of Optical Microscopy Images. It implements an image correction method based on low-rank and sparse decomposition to solve both shading in space and background variation in time. It can correct temporal drift in time-lapse microscopy data and thus improve continuous single-cell quantification. BaSiC is available as a Fiji/ImageJ plugin.</p>\r
\r
<p>&nbsp;</p>\r
""" ;
    ns1:featureList <http://edamontology.org/operation_Image_reconstruction>,
        <http://edamontology.org/operation_Image_stitching> ;
    ns1:license "Creative Commons Attribution-NonCommercial-NoDerivatives 4.0 International Public License" ;
    ns1:name "BaSiC" ;
    ns1:publisher "Carsten Marr",
        "Tingying Peng" ;
    ns1:softwareRequirements <http://biii.eu/node/79> .

